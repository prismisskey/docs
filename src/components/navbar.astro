---
const {title, ui} = Astro.props;
---



<style>
    p{
        margin:0px auto !important;
    }
    a{
        color: black !important;
    }
    .mx-auto{
        margin-left: auto;
        margin-right: auto;
        width: 79em;
    }
    .hamburger {
        display: none;
    }
    .sticky {
        background: #fafafa;
    }
    @media screen and (max-width: 636px) {
        .flex {
            max-width:1280px;
            width: 100%;
        }

        .hamburger {
            display: inline;
        }
    }

    @media (prefers-color-scheme: dark) {
        .sticky {
            background-color: #121212;
            color: #d9d9d9;
        }
        a{
            color: white !important;
        }
    }
</style>